<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Referral Rock</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/ReferralRock.Test.styles.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/general/general_usage.css" asp-append-version="true"/>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.0/font/bootstrap-icons.css" rel="stylesheet">

</head>
<body>
<header class="py-1 mb-5"> <!-- reduce the py-* value -->
    <div class="container">
        <a asp-area="" asp-controller="Members" asp-action="Index">Home</a>
    </div>
</header>


<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted sticky-footer centered-footer-content">
    <div class="container mb-3">
        <label class="mt-5">&copy; 2023 - ReferralRock.Test</label>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
